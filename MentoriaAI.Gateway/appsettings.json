{
  "ReverseProxy": {
    "Routes": {
      "cadastro": {
        "ClusterId": "cadastroCluster",
        "Match": { "Path": "/cadastro/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/cadastro" } ]
      },
      "busca": {
        "ClusterId": "buscaCluster",
        "Match": { "Path": "/busca/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/busca" } ]
      }
    },
    "Clusters": {
      "cadastroCluster": {
        "Destinations": {
          "cadastro": { "Address": "http://mentoria-cadastro:8080/" }
        }
      },
      "buscaCluster": {
        "Destinations": {
          "busca": { "Address": "http://mentoria-busca-semantica:8080/" }
        }
      }
    }
  }
}
